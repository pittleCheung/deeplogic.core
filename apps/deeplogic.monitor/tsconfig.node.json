{
  "compilerOptions": {
    "jsx": "react-jsx",
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"]
    },
    "types": ["vite/client"]
  },
  "include": ["vite.config.ts", "src/**/*"]
}
